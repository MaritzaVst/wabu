			text-decoration underline
			width 100%
			text-align center
			display block
input[type="password"]
	letter-spacing: 5px;
	font-size: .75em;
	height: 34px;
.lost-pwd
	.logo 
		margin-bottom 5em
	form .btn 
		margin-top 2em
.lost-pwd-sucess 
	p 
		text-align center
/*Buttons*/
.btn 
	color #1283ff;
	width: 200px;
	height: 40px;	
	border-radius 35px
	font-weight: 600	
.btn 
	transform: translateY(6px);
/*Main Content*/
.main-content
	float left 
	width 85%
	background-color: #ececf5;
	/*Listado*/
	.data-list 
		padding 2em
		table 
			width 100%
			border-collapse: separate;
			border-spacing: 0 .8em;
			th 
				font-weight: 400
				font-size: .835em
				text-align center
			thead
				th:first-child
					text-align left
					padding-left .5em
					
			tbody
				
				a 
					color #333
					display block
					position relative
				a.edit, a.view
					display inline-block
					padding .65em .5em
					margin 0 .5em 0 0
					text-decoration none
				a.view
					background  url('../img/ic-left-standby.png') 50% 50% no-repeat
				a.edit
					background  url('../img/ic-edit-copy.png') 50% 50% no-repeat
				tr td
					background-color: #FFF
					margin-bottom 1em
					padding: 1.25em 0 1.25em 0;
					position relative
					text-align center
					font-weight: 600
					font-size 1.2em
				tr 
					position relative
					box-shadow: 1px 3px 4px -1px rgba(0,0,0,0.31);
					background-color: #edecf6;
				tr.current td, tr:hover td
					background-color: #86C0FF
					transition-duration 0.35s
					a.edit 
						background  url('../img/ic-edit.png') 50% 50% no-repeat
					a.view 
						background  url('../img/ic-left-hover.png') 50% 50% no-repeat
				tr:hover td:last-child
					a:after
						background  url('../img/ic-left-hover.png') 50% 50% no-repeat
				tr td:last-child
					border-right: 1px solid rgba(0, 0, 0, 0.2);
					border-top-right-radius: 6px !important;
					border-bottom-right-radius: 6px !important;
					overflow-x: hidden;
				tr td:first-child
					width 350px
					border-top-left-radius 5px
					text-align left
					padding: 1.25em 0 1.25em 3em;
					border-top-left-radius: 6px !important;
					border-bottom-left-radius: 6px !important;
					overflow-x: hidden;
					a 
						display inline-block
				tr td:first-child:before
					content ""
					position absolute
					left 0 
					height 100%
					top 0
					background-color: #dedede
					width 25px
					transition-duration 0.3s
					cursor pointer
				tr.active td:first-child:before
					background-color: #1283ff
	.data-list.profesores 
		font-size: .9em
		table thead th:first-child
			width 25px
		table tbody tr td:first-child
			width auto
			padding: 0 0 0 2em;
			text-align center
		.pic 
			display inline-block 
			width 40px 
			height 40px 
			border 1px solid rgba(0, 0, 0, .1)
			border-radius 50%
			overflow hidden
		.foto 
			text-align center 
	.data-list.cursos
		table thead th:first-child
			width auto
			text-align center
	.data-list.admin-new > div 
		border-radius 4px
		background-color #ffffff
		box-shadow 0 2px 4px 0 rgba(0, 0, 0, 0.12)
		border solid 1px #e2dfdf		
		/*Administradores form style*/
		form 
			padding 2em 10em
			h4
				font-weight 400 
				text-transform uppercase
				font-size 13px
				color rgb(18, 131, 255)
				letter-spacing 0.3px
			/*New Level*/
			.img-preview
				#files
					position: absolute
					left: -9999px
					top: -9999px
					z-index: -9999
				.img-container
					width: 140px
					height: 140px
					border-radius: 8px
					background-color: #eaeaeb
					position: relative
					overflow: hidden
				span 
					letter-spacing: 0.3px
					color: #9b9b9b
					font-size: 12px
					text-transform: uppercase
					padding-left: 5px
				.upload-btn {
					margin-top 15px
					cursor pointer
				}
			/*------*/
			.content-form
				padding-bottom 40px
				.row 
					padding-top 12px 
					> div
						height 80px 
						label
							position absolute
							top 26px
							font-size 16px
							color rgba(90, 96, 102, 0.6)
							-webkit-transition 0.3s ease all
							transition 0.3s ease all
							&.active
								font-size 13px
								color rgb(18, 131, 255)
