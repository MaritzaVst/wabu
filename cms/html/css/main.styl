img 
	max-width 100%
body,html
	float left
	font-family: 'Lato', sans-serif;
	background-color: #40474e
	width 100% !important
input
	outline none
/*Utilities*/
.text-center
	text-align center
.text-left
	text-align left !important
.title-blue 
	font-size 24px
	font-weight 400
	color #1283ff
	text-transform uppercase
.btn-save
	color: #fff
	box-shadow: 0px 5px 8px -3px rgba(0,0,0,0.35)
	border: 0
	height: 49px
	width: 171px
	font-size: 13px
	text-transform: uppercase
	font-weight: 600
	border-radius: 25px
	margin: 0 1.5em
.btn-cancel
	border: 0px
	border-bottom: 2px solid rgba(0,0,0,0.2)
	padding: 1.15em 0 0.4em
	border-radius: 0
	color: #b3b3b3
	font-weight: bold
	text-transform: uppercase
	font-size: 12px
	letter-spacing: 0.3px
	margin: 0 1.5em
	background: transparent
	box-shadow: none
.token-input-dropdown
	border 0!important
	.alternative-text
		font-family 'lato'
		font-size 13px
		font-weight 400!important
		padding-top 5px
.absolute-center
	position: absolute
	transform: translate(-50%, -50%)
	top: 50%
	left: 50%
/*Errors messages*/
.parsley-errors-list
	display none
.parsley-error
	border-color: rgb(255, 63, 28) !important
	background-color: rgba(255, 255, 255, 0.23) !important
/*Loading*/
.loading 
	display none
	position absolute 
	top 0 
	left 0
	width 100%
	height 100%
	background-color: rgba(255, 255, 255, .5)
	.uil-reload-css
		position: absolute;
		left: 50%;
		margin-left: -100px;
/*Background Colors*/
.bg-white
	background-color: #FFF !important
.bg-green
	background-color: #00e6ae !important
/*Colors*/
.color-black
	color #40474e !important
	
.login
	display block
	height 100%
	background: url('../img/back.png')
	background-size: contain
	color #fff
	position relative
	min-height 100vh
	::-webkit-input-placeholder 
		color: white;
		opacity: .8;
		letter-spacing: 5px;
	::-moz-placeholder 
		color: white;
		opacity: .8;
	:-ms-input-placeholder 
		color: white;
		opacity: .8;
	:-moz-placeholder 
		color: white;
		opacity: .8;
		
	input[type="email"]::-webkit-input-placeholder
		font-size .75em
	input[type="email"]::-moz-placeholder
		font-size .75em
	input[type="email"]:-ms-input-placeholder
		font-size .75em
	input[type="email"]:-moz-placeholder
		font-size .75em
		
	.box 
		-webkit-transform: translateY(-50%);
		-ms-transform: translateY(-50%);
		transform: translateY(-50%);
		text-align center
		position absolute
		top: 50%;
		width 100%
	a 
		color #fff
	.logo 
		display block
		margin 0 auto 7em
		width 106px
	form 
		width 300px
		display inline-block
		margin 0 auto
		padding: 0 2em;
		text-align left
		p 
			margin-bottom 2.5em
			font-size: 1.15em
			font-weight: 300
		label,input, button
			display inline-block
			border 0	
			width 100%
			margin 0 0 .4em
			background-color: transparent
			color #fff
		label
			opacity: 0.45;
			margin-bottom  0
		input 
			padding .45em .5em .45em 0
			border-bottom 2px solid rgba(255, 255, 255, .5)
			margin-bottom 1.5em
			font-weight: bold
		.btn 
			color #1283ff;
			margin 0 auto 1em
			margin-top 4em
			max-width 200px
			display block
			box-shadow 0px 7px 8px -2px rgba(0, 0, 0, 0.18)
